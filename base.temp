<body bgcolor="#ffffff"
	style="font-family:Oxygen, Helvetica Neue, Arial, sans-serif!important;-webkit-font-smoothing:antialiased;-webkit-text-size-adjust:none;height:100%;color:#333333;width:100%!important;margin:0!important;">

	<table align="center" cellpadding="0" cellspacing="0" class="container-for-gmail-android"
		style="width:100%;min-width:600px;border-collapse:collapse!important;">

		<tr style="background-color:white;background:white; background-color:white;">

			<td align="left" valign="top"
				style="background:white;font-size:16px;color:#333333;text-align:center;line-height:24px;border-collapse:collapse;">

				<center>

					<table cellspacing="0" cellpadding="0" bgcolor="#ffffff"
						style="width:100%;background-color:transparent;border-collapse:collapse!important;">

						<tr>

							<td height="80" valign="top"
								style="text-align:center;vertical-align:middle;font-size:16px;color:#333333;line-height:24px;border-collapse:collapse;">

								<!--[if gte mso 9]>
									<v:rect xmlns:v="urn:schemas-microsoft-com:vml" fill="true" stroke="false" style="mso-width-percent:1000;height:80px;v-text-anchor:middle;">
										<v:fill type="tile" src="http://s3.amazonaws.com/swu-filepicker/4E687TRe69Ld95IDWyEg_bg_top_02.jpg" color="#ffffff" />
										<v:textbox inset="0,0,0,0"> 
								<![endif]-->

								<center>

									<!--//CABECERA//-->

									<table cellpadding="0" cellspacing="0" class=""
										style="width:635px!important;border-collapse:collapse!important;">

										<tr>

											<td class="pull-left mobile-header-padding-left"
												style="text-align:left;width:160px!important;padding-left:0!important;vertical-align:middle;font-size:16px;color:#333333;text-align:left;line-height:24px;border-collapse:collapse;">

												<a href="https://beseif.com" style="color:#676767;text-decoration:none!important;">

													<img width="137" height="auto"
														src="https://beseif.com/resources/images/marketplace/normal/Beseif.png"
														style="max-width:600px;outline:none;text-decoration:none;-ms-interpolation-mode:bicubic;border:none;">

												</a>
											</td>

											<td class="pull-right mobile-header-padding-right"
												style="text-align:right;width:160px!important;padding-right:0!important;color:#333333;font-size:16px;text-align:right;line-height:24px;border-collapse:collapse;">

												<a href="https://twitter.com/beseif_ES" style="color:#676767;text-decoration:none!important;">

													<img width="44" height="47"
														src="http://s3.amazonaws.com/swu-filepicker/k8D8A7SLRuetZspHxsJk_social_08.gif"
														style="max-width:600px;outline:none;text-decoration:none;-ms-interpolation-mode:bicubic;border:none;">

												</a>

												<a href="https://www.facebook.com/beseifES"
													style="color:#676767;text-decoration:none!important;">

													<img width="38" height="47"
														src="http://s3.amazonaws.com/swu-filepicker/LMPMj7JSRoCWypAvzaN3_social_09.gif"
														style="max-width:600px;outline:none;text-decoration:none;-ms-interpolation-mode:bicubic;border:none;">

												</a>

												<a href="https://blog.beseif.com/" style="color:#676767;text-decoration:none!important;">

													<img width="40" height="47"
														src="http://s3.amazonaws.com/swu-filepicker/hR33ye5FQXuDDarXCGIW_social_10.gif"
														style="max-width:600px;outline:none;text-decoration:none;-ms-interpolation-mode:bicubic;border:none;">

												</a>
											</td>
										</tr>
									</table>

								</center>
								<!--[if gte mso 9]> </v:textbox> </v:rect> <![endif]-->
							</td>
						</tr>
					</table>
				</center>
			</td>
		</tr>
		<tr>
			<td
				style="background-color:#eeeeee;font-size:16px;color:#333333;text-align:center;line-height:24px;border-collapse:collapse;padding:0px;">
				<center>
					<table cellspacing="0" cellpadding="0" class=""
						style="width:100%;background-color:white;width:635px!important;border-collapse:collapse!important;">

						<!-- TITLE -->
						<tr>
							<td class="header-lg"
								style="word-break:break-word;background-color:#f6f6f6;text-align:center;font-size:32px;font-weight:700;line-height:normal;padding:45px 60px 45px 60px;color:#333333;">
								{{title}}
							</td>
							<td class="line-height:0px;height:0px;"> </td>
						</tr>
						<!-- END: TITLE -->

						<!-- MAIN CONTENT -->
						<tr style="text-align:left;">
							<td
								style="padding:25px 40px 10px 40px;font-size:16px;color:#333333;text-align:left;line-height:24px;border-collapse:collapse;width:100%!important;">
								{{#each main}}
								<p>{{this}}</p>
								{{/each}}
							</td>
						</tr>
						<!-- END: MAIN CONTENT -->

						<!-- BUTTON -->
						<tr>
							<td class="button"
								style="margin-bottom:20px;padding:10px 0px;font-size:16px;color:#333333;text-align:center;line-height:24px;border-collapse:collapse;">

								<table cellspacing="0" cellpadding="0" class="w320"
									style="width:635px!important;border-collapse:collapse!important;">

									<tr>
										<td class="button">
											<center style="text-align:center;">
												<a href="{{button.url}}"
													style="padding:10px 15px;border:1px solid #19a9d0;border-radius:5px;color:#19a9d0;display:inline-block;font-weight:regular;text-align:center;text-decoration:none;">
													{{button.text}}
												</a>
											</center>
										</td>
									</tr>
								</table>
							</td>
						</tr>
						<!-- END: BUTTON -->


						<!-- CARD -->
						{{#if card}}
						<tr>
							<td class="w320" style="width:635px!important;">
								<br />
								<table cellpadding="0" cellspacing="0" style="width:100%">
									<tr>
										<td class="mini-container-left" style="width:278px;padding:0px 40px 20px 40px;">
											<table cellpadding="0" cellspacing="0" style="width:100%">
												<tr>
													<td class="mini-block-padding">
														<table cellspacing="0" cellpadding="0"
															style="width:100%;border-collapse:separate!important;">
															<tr>
																<td class="mini-block"
																	style="border:1px solid #e5e5e5;border-radius:5px;padding:30px;text-align:left;width:253px;font-size:16px;color:#333333;line-height:24px;border-collapse:collapse;background-color:#ffffff;">
																	{{#each card}}
																	<p class="header-sm"
																		style="font-size:18px;font-weight:700;line-height:normal;color:#333333;">
																		{{this.title}}
																	</p>
																	{{#each this.content}}
																	<p>{{this}}</p>
																	{{/each}}
																	{{/each}}
																</td>
															</tr>
														</table>
													</td>
												</tr>
											</table>
										</td>
									</tr>
								</table>
							</td>
						</tr>
						{{/if}}
						<!-- END: CARD -->

						<!-- DETAILS -->
						{{#if details}}
						{{#with details}}
						<tr>
							<td class="w320" style="width:635px !important;"> <br />
								<table cellspacing="0" cellpadding="0"
									style="width:100%;background-color:white;border-collapse:collapse !important;">
									<tr>
										<td class="mini-container-left" style="background-color:#ffffff;width:100%;padding:20px 40px;">
											<table cellpadding="0" cellspacing="0" style="width:100%;">
												<tr>
													<td>
														<table cellpadding="0" cellspacing="0" style="width:100%">
															<tr>
																<td
																	style="text-align:left;padding-bottom:5px;color:#19a9d0;font-size:16px;line-height:24px;border-collapse:collapse;border-bottom:1px solid #e5e5e5;">
																	<h4 style="color:#19a9d0;margin-bottom:10px;">
																		Detalles del pedido
																	</h4>
																</td>
															</tr>
														</table>
													</td>
												</tr>
											</table>

											<table cellpadding="0" cellspacing="0" style="width:100%;">
												<tr>
													<td style="border:none;border-radius:5px;">
														<table cellspacing="0" cellpadding="0"
															style="width:100%;border-collapse:separate !important;">
															<tr>
																<td
																	style="padding:10px 0px 20px 0px;text-align:left;width:auto;font-size:16px;color:#333333;line-height:24px;border-collapse:collapse;background-color:#ffffff;border:none;">

																	<!-- ITEM -->
																	{{#each items}}
																	<table cellspacing="0" cellpadding="0"
																		style="width:100%;margin:20px 0px 25px 0px;table-layout:fixed;background-color:white;border-collapse:collapse !important;">
																		<tr>
																			<td style="text-align:left;padding-right:20px;width:24%;">
																				<img
																					style="width:100px;height:100px;object-fit:cover;border-radius:5px;margin-right:20px;outline:none;text-decoration:none;-ms-interpolation-mode:bicubic;border:none;"
																					src="{{this.image}}" />
																			</td>
																			<td style="width:50%;">
																				<table cellspacing="0" cellpadding="0"
																					style="width:100%;table-layout:fixed;background-color:white;border-collapse:collapse !important;">
																					<tr>
																						<td class="item-col"
																							style="font-weight:600;width:100%;text-align:left;vertical-align:top;font-size:16px;color:#333333;text-align:left;line-height:24px;border-collapse:collapse;padding-top:0px;vertical-align:top;">
																							{{this.name}}
																						</td>
																					</tr>
																				</table>
																				<table cellspacing="0" cellpadding="0"
																					style="width:100%;table-layout:fixed;background-color:white;border-collapse:collapse !important;">
																					<tr>
																						<td class="item-col"
																							style="width:100%;text-align:left;vertical-align:top;font-size:16px;color:#333333;text-align:left;line-height:24px;border-collapse:collapse;padding-top:0px;vertical-align:top;">
																							{{this.description}}
																						</td>
																					</tr>
																				</table>
																			</td>
																			<td style="width:26%;">
																				<table cellspacing="0" cellpadding="0"
																					style="width:100%;table-layout:fixed;background-color:white;border-collapse:collapse !important;">
																					<tr>
																						<td style="text-align: right">
																							{{this.price}}
																						</td>
																					</tr>
																				</table>
																			</td>
																		</tr>
																	</table>
																	{{/each}}
																	<!-- END: ITEM -->

																	<!-- ADDITIONAL -->
																	{{#each additional}}
																	<table cellspacing="0" cellpadding="0"
																		style="width:100%;border-collapse:separate !important;{{#unless @index}}border-top:1px solid #e5e5e5;{{/unless}}">
																		<tr>
																			<td
																				style="padding:0px;text-align:left;width:auto;font-size:16px;color:#333333;border-collapse:collapse;background-color:#ffffff;">

																				<table cellspacing="0" cellpadding="0"
																					style="width:100%;background-color:white;border-collapse:collapse !important;">
																					<tr>
																						<td class="item-col"
																							style="width:40%;min-width:220px;font-weight:bold;padding-top:15px;text-align:left;vertical-align:top;font-size:16px;color:#333333;text-align:left;line-height:24px;border-collapse:collapse;vertical-align:top;">
																							{{this.name}}
																						</td>
																						<td class="item-col2"
																							style="padding-top:15px;text-align:right;vertical-align:top;font-size:16px;color:#333333;line-height:24px;border-collapse:collapse;vertical-align:top;">
																							{{this.value}}
																						</td>
																					</tr>
																				</table>
																			</td>
																		</tr>
																	</table>
																	<table cellspacing="0" cellpadding="0"
																		style="width:100%;border-collapse:separate !important;">
																		<tr>
																			<td
																				style="padding:0px;text-align:left;width:auto;font-size:16px;color:#333333;border-collapse:collapse;background-color:#ffffff;">

																				<table cellspacing="0" cellpadding="0"
																					style="width:100%;background-color:white;border-collapse:collapse !important;">
																					<tr>
																						<td class="item-col"
																							style="width:40%;min-width:220px;font-weight:bold;padding-top:15px;text-align:left;vertical-align:top;font-size:16px;color:#333333;text-align:left;line-height:24px;border-collapse:collapse;vertical-align:top;">
																							{{this.name}}
																						</td>
																						<td class="item-col2"
																							style="padding-top:15px;text-align:right;vertical-align:top;font-size:16px;color:#333333;line-height:24px;border-collapse:collapse;vertical-align:top;">
																							{{this.value}}
																						</td>
																					</tr>
																				</table>
																			</td>
																		</tr>
																	</table>
																	{{/each}}
																	<!-- ADDITIONAL -->
																</td>
															</tr>
														</table>
													</td>
												</tr>
											</table>
										</td>
									</tr>
								</table>
							</td>
						</tr>
						{{/with}}
						{{/if}}
						<!-- END: DETAILS -->
					</table>
			</td>
		</tr>

		<tr>
			<td class="footer"
				style="background-color:#f6f6f6;font-size:16px;color:#333333;text-align:center;line-height:24px;border-collapse:collapse;">
				<center>
					<table cellspacing="0" cellpadding="0" class="w320"
						style="width:100%;background-color:#f6f6f6;width:635px!important;border-collapse:collapse!important;">
						<tr>
							<td>
								<center style="padding:25px 0 25px; color:#676767;">
									<div>
										<strong>Atenci√≥n al cliente</strong>
									</div>
									<div>
										Email: <a href="mailto:hello@beseif.com" target="_blank"
											style="color:#676767;text-decoration:none!important;">hello@beseif.com</a>
									</div>
									<div>
										<a href="https://beseif.com" target="_blank"
											style="color:#676767;text-decoration:none!important;">www.beseif.com</a>
									</div>
								</center>
							</td>
						</tr>
					</table>
				</center>
			</td>
		</tr>
	</table>

</body>

</html>